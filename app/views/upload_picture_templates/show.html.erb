<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Upload picture #<%= @upload_picture.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/upload_pictures" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link upload_picture start -->
      <div class="col">
        <a href="/upload_pictures/<%= @upload_picture.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit upload picture
        </a>
      </div>
      <!-- Edit link upload_picture end -->

    </div>

    <dl>
      <dt>
        Email
      </dt>
      <!-- Display email start -->
      <dd>
        <%= @upload_picture.email %>
      </dd>
      <!-- Display email end -->

      <dt>
        Password
      </dt>
      <!-- Display password start -->
      <dd>
        <%= @upload_picture.password %>
      </dd>
      <!-- Display password end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@upload_picture.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@upload_picture.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->

<!-- Show Page Customization end -->
